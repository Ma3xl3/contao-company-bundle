{
  "name":"oveleon/contao-company-bundle",
  "type":"contao-bundle",
  "description":"Company integration for Contao 4 Open Source CMS",
  "keywords":["contao","company-bundle"],
  "homepage":"https://www.oveleon.de/",
  "license":"MIT",
  "authors":[
    {
      "name":"Oveleon",
      "homepage":"https://oveleon.de/",
      "role":"Developer"
    }
  ],
  "require":{
    "php":"^7.1",
    "contao/core-bundle":"^4.4",
    "menatwork/contao-multicolumnwizard-bundle": "^3.4"
  },
  "require-dev": {
    "contao/manager-plugin": "^2.0"
  },
  "conflict": {
    "contao/core": "*",
    "contao/core-bundle": "4.4.1",
    "contao/manager-plugin": "<2.0 || >=3.0"
  },
  "autoload":{
    "psr-4": {
      "Oveleon\\ContaoCompanyBundle\\": "src/"
    },
    "classmap": [
      "src/Resources/contao/"
    ],
    "exclude-from-classmap": [
      "src/Resources/contao/config/",
      "src/Resources/contao/dca/",
      "src/Resources/contao/languages/",
      "src/Resources/contao/templates/"
    ]
  },
  "extra":{
    "branch-alias": {
      "dev-master": "1.2.x-dev"
    },
    "contao-manager-plugin": "Oveleon\\ContaoCompanyBundle\\ContaoManager\\Plugin"
  }
}